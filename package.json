{
  "name": "fluxxor",
  "version": "1.7.3",
  "description": "Flux architecture tools for React",
  "repository": {
    "type": "git",
    "url": "https://github.com/BinaryMuse/fluxxor.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive",
    "build": "./script/build-fluxxor",
    "lint-staged": "$(npm bin)/lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "lib/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": [
    "test",
    "lint-staged"
  ],
  "keywords": [
    "react",
    "flux"
  ],
  "author": "Michelle Tilley <michelle@michelletilley.net>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "chai": "^4.1.2",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jsdom": "~3.1.2",
    "lint-staged": "^4.2.3",
    "mocha": "^4.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.7.4",
    "react": "^0.13.0",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.5.0",
    "webpack": "^1.1.11",
    "webpack-dev-server": "^1.2.7"
  },
  "dependencies": {
    "eventemitter3": "^2.0.3",
    "lodash": "^4.17.4",
    "object-path": "^0.11.4"
  }
}
